---
import Hero from "../Hero.astro";
import BlockRenderer from "../BlockRenderer.astro";

type Props = {
  content: any;
};

const { content } = Astro.props;

const projectFields = content.projectFields.details;
const contentFields = content.contentFields.content;
---

<Hero content={projectFields} />
<article class="max-w-[64rem] flex flex-col mx-auto size-full">
  <BlockRenderer blocks={contentFields} />
</article>
